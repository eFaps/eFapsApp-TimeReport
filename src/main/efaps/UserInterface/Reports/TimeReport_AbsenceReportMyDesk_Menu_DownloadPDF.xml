<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>d6cd3e55-4a7f-49db-8967-741e4dee01bf</uuid>
    <file-application>eFapsApp-TimeReport</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>TimeReport_AbsenceReportMyDesk_Menu_DownloadPDF</name>
        <icon>Document_PDF</icon>
        <access>
            <role>Sales_Admin</role>
        </access>
        <target>
            <execute method="exportReport" program="org.efaps.esjp.timereport.reports.AbsenceReport">
                <property name="Mime">pdf</property>
                <property name="PageType">A4</property>
                <property name="PageOrientation">LANDSCAPE</property>
                <property name="FilterKey">org.efaps.esjp.timereport.reports.AbsenceReport.FilterKey</property>
                <property name="Type">Sales_RetentionCertificate</property>
                <property name="StatusGroup">Sales_RetentionCertificateStatus</property>
                <property name="Status">!Canceled</property>
            </execute>
        </target>
        <property name="Target">hidden</property>
        <property name="TargetShowFile">true</property>
        <property name="Label">report.DownloadPDF</property>
    </definition>
</ui-command>
